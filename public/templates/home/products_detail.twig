{% extends 'frontbase.html.twig' %}

{% block title %} {{ data[0].title }}{% endblock %}

{% block keywords %}  {{ data[0].keywords }} {% endblock %}
{% block description %} {{ data[0].description }}{% endblock %}
{% block sidebar %} {{ include ('home/sidebar.html.twig') }} {% endblock %}
{% block content %}
<div>
    <head >
        <link rel="icon"
              type="image/png"

              href="https://cdn4.iconfinder.com/data/icons/natural-green-perfect-pixel-2/512/Leaves_leaf_green_natural-512.png">

</head>

<style>
    
           .prod{ 
          display: block;
 max-height: 345.6px;
 max-width: 240px;
 min-height:275px ;
 min-width:200px ;
 
 object-fit: scale-down;
 
}


td,tr,table,tbody
{ border:1px solid;}
    
</style>

<div class="wrap">
    


        <div class="product-details"><!--product-details-->
            <div class="col-sm-5" >
                <div class="view-product" >



                    <div class="carousel slide" id="similar-product" data-ride="carousel" >

                        <!-- Wrapper for slides -->

                        <div class="carousel-inner" >

                            <div class="item active" >
                                <a href=""> <img class="prod" alt="" src="{{ asset('uploads') }}/images/{{ data[0].image }}"  height="200px" width="200"></a>

                            </div>

                            {% for rs in image %}

                                <div class="item">
                                    <a href=""><img alt="" src="{{ asset('uploads') }}/images/{{ rs.image }}" height="200px" width="200"></a>
                                </div>
                            {% endfor %}
                        </div>




                        <!-- Controls -->
                        
                    </div>
                    
                    
                    
                    <h3>
                        
                           {% if app.request.getLocale() == 'tr'%} {{ data[0].titletr }}
 {% elseif app.request.getLocale() == 'ar'%}
{{ data[0].titlear }}
 
 {% else %}
 
 {{ data[0].title }}

                      {% endif %}
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        
                        </h3>
                </div>


            </div>
            <div class="col-sm-7">
                <div class="product-information"><!--/product-information-->

                    <h2>
                         {% if app.request.getLocale() == 'tr'%} {{ data[0].titletr }}
                         {% elseif app.request.getLocale() == 'ar'%}
                        {{ data[0].titlear }}
 
 {% else %}
 
 {{ data[0].title }}
                                      {% endif %}
                        
                        </h2>
                    <p><b>{% trans %}Product ID:{% endtrans %}</b> {{ data[0].id }}</p>
                    <br>
                    <p><b>{% trans %}Product Description:{% endtrans %}</b>
                    
                    
                                               {% if app.request.getLocale() == 'tr'%} {{ data[0].descriptiontr }}
 {% elseif app.request.getLocale() == 'ar'%}
{{ data[0].descriptionar }}
 
 {% else %}
 
    {{ data[0].description }}

                      {% endif %}
                    
                    
                    
                </p>




                    <p><b>{% trans %}Type:{% endtrans %}</b>
                    
                                                               {% if app.request.getLocale() == 'tr'%}{{ data[0].typetr }}
 {% elseif app.request.getLocale() == 'ar'%}
{{ data[0].typear }}
 
 {% else %}
 
    {{ data[0].type }}

                      {% endif %}
                    
                    
                    
                    
                    
                    </p>
                    
                    
                       {% if data[0].type == "list" or data[0].type == "product with brochur" %} 
                                    <br>
                    <a class="btn btn-default add-to-cart" style="background-color: #FE980F; color: white; margin-bottom: 5px;"  href="{{ asset('uploads') }}/{{data[0].title}}.pdf" download >{% trans  %}Download PDF File{% endtrans %} <i class="fa fa-file-pdf-o"></i> </a>
                    <p> <b> {% trans %}Or{% endtrans %}</b></p> 
                     <a style="background-color: #FE980F; color: white" class="btn btn-default add-to-cart" href="{{ asset('uploads') }}/{{data[0].title}}.pdf" target="_blank" >{% trans  %}View Brouchure{% endtrans %} <i class="fa fa-file-pdf-o"></i> </a>
                  
                       
                       {% endif%}
                       
                    
                

                </div><!--/product-information-->
            </div>
        </div><!--/product-details-->
        
        
        
        
     {% if data[0].type == "list" %} 
     
     </div>
     
     
     
     
     {% else %}



 

