<div class="wrap">
<div class="header">
    <div class="headertop_desc">
        <div class="call">
            <p><span>{% trans  %}Need help? {% endtrans %}</span>  {% trans  %} call us {% endtrans %}<span class="number"> 00905366929481</span></p>
        </div>
        <div class="account_desc">
            <ul style="font-size: 150%">
                <li><a href="{{ path(app.request.get('_route'), app.request.get('_route_params')|merge({'_locale': 'tr'})) }}">Türkçe</a></li>
                <li><a href="{{ path(app.request.get('_route'), app.request.get('_route_params')|merge({'_locale': 'en'})) }}">English</a></li>
