                <li><a href="{{ path('app_login') }}">Login</a></li>
                <li><a href="{{ path('shopcart_index') }}">ShopCart</a></li>
                <li><a href="{{ path('userpanel') }}">My Account</a></li>

            </ul>
        </div>
        <div class="clear"></div>
    </div>
    <div class="header_top" style="background-color: white">
        <div class="logo">

            <a href=""><img alt="" height="20%" width="50%" src="{{ asset('uploads') }}/images/rosalo.png"></a>


        </div>


        <div class="clear"></div>
    </div>
    {{ include ('admin/messages.html.twig') }}
    <div class="header_bottom" style="font-size: 110%">
        <div class="menu" style="z-index: 10">
            <ul>
                <li ><a href="{{ path('home') }}">{% trans  %}Home{% endtrans %}</a></li>
                <li><a href="{{ path('aboutus') }}">{% trans  %}About{% endtrans %}</a></li>

                <li><a href="{{ path('contact') }}">{% trans %}Contact{% endtrans %}</a></li>
                <li><a href="{{ path('reference') }}">{% trans %}References{% endtrans %}</a></li>




            </ul>



        </div>
        {% if is_granted('ROLE_ADMIN') %}



            <nav id="primary_nav_wrap" >
                <ul>

                    <li><a href="#"> Logged in As: {{ app.user.name }}</a>
                        <ul>
                            <li><a href="{{ path('admin') }}">Admin Panel</a></li>
