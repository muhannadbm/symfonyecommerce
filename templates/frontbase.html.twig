                            <li><a href="{{ path('app_logout') }}">Log Out</a></li>
                        </ul>

                    </li>

                </ul>
                {% elseif is_granted('ROLE_USER') %}

                <nav id="primary_nav_wrap"  >
                    <ul>

                        <li><a href="#"> Logged in As: {{ app.user.name }}</a>
                            <ul>

                                <li><a href="{{ path('app_login') }}">My User Page</a></li>
                                <li><a href="{{ path('app_logout') }}">Log Out</a></li>
                            </ul>

                        </li>

                    </ul>
        {% else %}
            <nav id="primary_nav_wrap">
                <ul>

                   <li><a href="{{ path('app_login') }}">Login</a> </li>
                    <li><a href="{{ path('new_user') }}">Register </a> </li>
                   </ul>
            </nav>
            </li>/
            </ul>


        {% endif %}
        <div class="clear"></div>
    </div>

</div>
